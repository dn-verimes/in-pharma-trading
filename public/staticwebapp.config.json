{
  "routes": [
    {
      "route": "/",
      "redirect": "/en/"
    },
    {
      "route": "/en/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/de/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/nl/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/en/index.html",
    "exclude": ["/images/*", "/*.{js,css,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}", "/api/*"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/en/404.html"
    }
  }
}
